{
  "manifest_version": 3,
  "name": "Udemy Note Taker",
  "description": "Take notes on Udemy, view community notes, and share your notes.",
  "version": "1.0",
  "permissions": ["storage", "identity", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.udemy.com/course/*"],
      "js": ["content.js"],
      "css": ["sidebar.css"]
    }
  ],
  "oauth2": {
    "client_id": "740880158839-he6q8ft4qbe8mikkedfl65udu6763mdt.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "key": "",
  "host_permissions": ["https://www.udemy.com/*", "http://localhost:3001/*"],
  "web_accessible_resources": [
    {
      "resources": ["sidebar.html", "icons/*"],
      "matches": ["https://www.udemy.com/*"]
    }
  ]
}

